{
    "default": {
        "tags": [
            " @order-1",
            " @order-2",
            " @order-3",
            " @order-4",
            " @order-5",
            " @order-6 "
        ],
        "formatOptions": {
            "snippetInterface": "async-await"
        },
        "paths": [
            "src/test/features/"
        ],
        "publishQuiet": true,
        "dryRun": false,
        "require": [
            "src/test/steps/*.ts",
            "src/hooks/hooks.ts"
        ],
        "requireModule": [
            "ts-node/register"
        ],
        "format": [
            "progress-bar",
            "html:test-results/cucumber-report.html",
            "json:test-results/cucumber-report.json",
            "rerun:@rerun.txt"
        ]
    },
    "rerun": {
        "formatOptions": {
            "snippetInterface": "async-await"
        },
        "publishQuiet": true,
        "dryRun": false,
        "require": [
            "src/test/steps/*.ts",
            "src/hooks/hooks.ts"
        ],
        "requireModule": [
            "ts-node/register"
        ],
        "format": [
            "progress-bar",
            "html:test-results/cucumber-report.html",
            "json:test-results/cucumber-report.json",
            "rerun:@rerun.txt"
        ]
    }
}